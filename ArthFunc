int mul (int a, int b);
int sub (int a, int b);
int add (int a, int b);

int main ()
{
 a=10;
 b=20;
 c=mul (a,b);
 cout << c << endl;
 c=sub (a,b);
 cout << c << endl;
 c=add (a,b);
 cout << c << endl;
 return 0;
}

int mul (int a, int b)
{
 return a*b;
}

int sub (int a, int b)
{
 return a-b;
}

int add (int a, int b)
{
 return a+b;
}